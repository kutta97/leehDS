#include<iostream>
#include<limits.h>

using namespace std;

int main() {
	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int tmp, N;
	int min = INT_MAX;
	int max = INT_MIN;
	
	cin >> N;

	for (int i = 0; i < N; i++) {
		cin >> tmp;
		if (tmp > max)
			max = tmp;
		if (tmp < min)
			min = tmp;
	}
	cout << min << " " << max;

}
